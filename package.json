{
  "name": "ics-merger-screenshot-plugin",
  "version": "0.1.0",
  "main": "dist/bundle.js",
  "repository": "https://github.com/jacobmischka/ics-merger-screenshot-plugin.git",
  "author": "Jacob Mischka <jacob@mischka.me>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "web-ext run",
    "watch": "run-p watch:*",
    "watch:background": "yarn build:background --watch",
    "watch:content": "yarn build:content --watch",
    "build": "run-p build:*",
    "build:background": "rollup --sourcemap --input 'src/background.js' --output.file 'dist/background.js' --output.format iife",
    "build:content": "rollup --sourcemap --input 'src/content.js' --output.file 'dist/content.js' --output.format iife",
    "build:webext": "web-ext build --overwrite-dest"
  },
  "devDependencies": {
    "eslint": "^4.7.1",
    "npm-run-all": "^4.1.1",
    "rollup": "^0.50.0",
    "web-ext": "^2.0.0"
  },
  "dependencies": {}
}
